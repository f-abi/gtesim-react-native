{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ],
      "@/stores/*": [
        "./stores/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/locales/*": [
        "./locales/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "types/*.d.ts",
    "nativewind-env.d.ts"
  ]
}